{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "nsis"
    ],
    "windows": {
      "nsis": {
        "installMode": "currentUser"
      }
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "JX3RaidManager",
  "mainBinaryName": "JX3RaidManager",
  "version": "2.1.0",
  "identifier": "com.jx3.raidmanager",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "剑网三副本管家",
        "width": 1200,
        "height": 800,
        "decorations": true,
        "transparent": false,
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": "default-src 'self' 'unsafe-eval' 'unsafe-inline'; style-src 'self' 'unsafe-inline'; font-src https://fonts.googleapis.com; img-src 'self' data: https://icon.jx3box.com; connect-src ipc: http://ipc.localhost 'self' https://open.bigmodel.cn https://generativelanguage.googleapis.com https://node.jx3box.com"
    }
  }
}